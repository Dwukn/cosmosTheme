{
  "manifest_version": 2,
  "name": "SM",
  "version": "1.0",
  "description": "Replace new tab with a custom tab",
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "permissions": [
    "contextMenus"
  ],
  "browser_action": {
    "default_icon": {
      "48": "images/icon.png"
    },
    "default_title": "Open Extension Dashboard",
    "default_popup": "main.html"
  },
  "web_accessible_resources": [
    "styles.css",
    "script.js",
    "editor.js",
    "images/space.jpg",
   "settings/help.html"
  ],
  "icons": {
    "48": "images/icon.png"
  }
}
